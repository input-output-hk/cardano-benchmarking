{
  "LastKnownBlockVersion-Alt": 0,
  "LastKnownBlockVersion-Major": 0,
  "LastKnownBlockVersion-Minor": 2,
  "Protocol": "RealPBFT",
  "RequiresNetworkMagic": "RequiresMagic",

  "TurnOnLogging": true,
  "TurnOnLogMetrics": false,
  "setupBackends": [
    "KatipBK"
  ],
  "setupScribes": [
    {
      "scFormat": "ScJson",
      "scKind": "FileSK",
      "scName": "logs/generator.json"
    },
    {
      "scFormat": "ScText",
      "scKind": "StdoutSK",
      "scName": "stdout",
      "scRotation": null
    }
  ],
  "defaultBackends": [
    "KatipBK"
  ],
  "defaultScribes": [
    [
      "FileSK",
      "logs/generator.json"
    ],
    [
      "StdoutSK",
      "stdout"
    ]
  ],

  "minSeverity": "Debug",

  "TraceBlockFetchClient": true,
  "TraceBlockFetchDecisions": false,
